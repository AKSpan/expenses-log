<div class="center-block__info-block calculator-window">
    <div class="calculator-window__back-wrapper"><input name="back-button" value="" title="Back" type="button"
                                                        class="info-row__action-button"></div>
    <p>Калькулятор</p>
    <div class="calculator-window__done-calc-wrapper"><input name="done-button" value="" title="Done" type="button"
                                                             class="info-row__action-button"></div>
</div>
<div class="center-block__info-block">
    <input class="calc-result" readonly value="" maxlength="15">
    <div class="calc-row calc-row-4">
        <input class="calc-button calc-button-2" id="calc-clear" value="C" type="button" action>
        <input class="calc-button calc-button-1" id="calc-del" value="/" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-mul" value="*" type="button" symbol>
    </div>
    <div class="calc-row calc-row-4">
        <input class="calc-button calc-button-1" id="calc-7" value="7" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-8" value="8" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-9" value="9" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-minus" value="-" type="button" symbol>
    </div>
    <div class="calc-row calc-row-4">
        <input class="calc-button calc-button-1" id="calc-4" value="4" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-5" value="5" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-6" value="6" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-plus" value="+" type="button" symbol>
    </div>
    <div class="calc-row calc-row-3">
        <input class="calc-button calc-button-1" id="calc-1" value="1" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-2" value="2" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-3" value="3" type="button" symbol>
    </div>
    <div class="calc-row calc-row-vert-2">
        <input class="calc-button calc-button-1" id="calc-equal" value="=" type="button" action>
    </div>
    <div class="calc-row calc-row-3 calc-last-row ">
        <input class="calc-button calc-button-2" id="calc-0" value="0" type="button" symbol>
        <input class="calc-button calc-button-1" id="calc-dot" value="." type="button" symbol>
    </div>

</div>